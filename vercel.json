{
    "builds": [
        {
            "src": "main.py",
            "use": "@vercel/python"
        }
    ],
    "routes": [
        {
            "src": "/quiz",
            "dest": "main.py"
        },
        {
            "src": "/webhook",
            "dest": "main.py"
        },
        {
            "src": "/tilda-webhook",
            "dest": "main.py"
        },
        {
            "src": "/programs",
            "dest": "main.py"
        },
        {
            "src": "/health",
            "dest": "main.py"
        },
        {
            "src": "/(.*)",
            "dest": "main.py"
        }
    ]
}